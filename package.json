{
  "name": "luminosia",
  "description": "Luminosia Studio",
  "version": "0.0.0-semantic-release",
  "homepage": ".",
  "homepage-web": "https://gmoehler.github.io/luminosia",
  "license": "MIT",
  "author": {
    "name": "Gregor Moehler",
    "email": "gmoeh@gmx.de"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "@material-ui/styles": "4.10.0",
    "download-github-release": "0.3.2",
    "electron-devtools-installer": "3.1.1",
    "electron-is-dev": "^1.2.0",
    "fade-maker": "1.0.3",
    "fast-deep-equal": "2.0.1",
    "file-saver": "2.0.2",
    "gh-pages": "2.0.1",
    "i": "0.3.6",
    "input-range-scss": "1.5.1",
    "is-electron": "^2.2.0",
    "log-scale": "1.0.1",
    "mdi-material-ui": "6.20.0",
    "memoize-one": "5.1.1",
    "normalizr": "3.6.1",
    "promisify-child-process": "^3.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "react-scripts": "4.0.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "styled-components": "5.2.1",
    "tmp": "^0.2.1",
    "tree-kill": "^1.2.1",
    "webaudio-peaks": "^0.0.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "electron": "rescripts start",
    "electron-build": "rescripts build",
    "electron-test": "rescripts test",
    "electron-dev": "concurrently \"yarn run electron\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn electron-build",
    "electron-pack": "build --win --linux",
    "semantic-release": "semantic-release",
    "commitmsg": "validate-commit-msg"
  },
  "main": "public/electron.js",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@rescripts/cli": "^0.0.14",
    "@rescripts/rescript-env": "0.0.12",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/npm": "^5.1.4",
    "@storybook/addon-actions": "4.1.11",
    "@storybook/react": "4.1.11",
    "@testing-library/react": "^11.1.2",
    "concurrently": "^4.1.0",
    "electron": "10.1.5",
    "electron-builder": "^22.9.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "14.2.1",
    "husky": "2.2.0",
    "react-test-renderer": "17.0.1",
    "redux-mock-store": "1.5.4",
    "semantic-release": "^17.2.2",
    "typescript": "^3.4.5",
    "validate-commit-message": "3.0.1",
    "wait-on": "^3.2.0"
  },
  "build": {
    "appId": "com.luminosia.studio",
    "productName": "LuminosiaStudio",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "linux": {
      "target": [
        {
          "target": "deb",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "extraResources": [
      {
        "from": "./resources/linux",
        "to": "linux",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "./resources/win32",
        "to": "win32",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-message"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "assets": [
            "dist/LuminosiaStudio*",
            "dist/luminosia*"
          ]
        }
      ]
    ]
  }
}
